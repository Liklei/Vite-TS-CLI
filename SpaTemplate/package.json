{
  "name": "spa_template",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Lance",
    "email": "lkl641818@gmail.com"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "cross-env NODE_ENV=production vite build --mode production && esno ./build/script/buildServer.ts",
    "build:test": "cross-env NODE_ENV=test vite build --mode test && esno ./build/script/buildServer.ts",
    "build:env": "vue-tsc --noEmit && vite build",
    "build:no-cache": "npm clean:cache && npm run build",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview --port 8080",
    "preview:dist": "vite preview",
    "prepare": "husky install",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "test:unit": "jest",
    "clean:lib": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "reinstall": " npm run clean:lib && npm run clean:cache && npm i"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons-vue": "^6.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.25",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.33",
    "@vueuse/core": "^8.3.1",
    "@vueuse/head": "^0.7.6",
    "ant-design-vue": "^3.2.0",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.13",
    "qs": "^6.10.3",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.30",
    "vue-router": "4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "esno": "^0.14.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.4.0",
    "mockjs": "^1.1.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.12",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-theme": "^0.8.6",
    "vite-plugin-vue-images": "^0.6.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.4",
    "vue-global-api": "^0.4.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.34.7",
    "windicss": "^3.5.1"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
